args=( ${(Q)"${(z)$(<<\EO:args
'(--revision)-r+:arg:' '(-r)--revision=:arg:' '(--quiet)-q' '(-q)--quiet' '(--non-recursive)-N' '(-N)--non-recursive' '--depth=:operation depth (how far to recurse):(empty files immediates infinity)' '--force' '--native-eol=:arg:' '--ignore-externals' '--ignore-keywords' '--username=:arg:' '--password=:arg:' '--password-from-stdin' '--no-auth-cache' '--non-interactive' '--force-interactive' '--trust-server-cert' '--trust-server-cert-failures=:failures:_values -s , "certificate failures to ignore" "unknown-ca[unknown authority]" "cn-mismatch[hostname mismatch]" "expired[certificate expired]" "not-yet-valid[certificate not yet valid]" "other[all other failures]"' '--config-dir=:config dir:_directories' '*--config-option=: :_svn_config_options'
EO:args
)}"} )
